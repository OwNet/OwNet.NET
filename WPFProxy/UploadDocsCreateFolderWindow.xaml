<Window x:Class="WPFProxy.UploadDocsCreateFolderWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Create a new folder" Height="145" Width="320">
    <Grid>
        <Label Content="Enter the folder name:" Height="28" HorizontalAlignment="Left" Name="label1" VerticalAlignment="Top" Margin="12,12,0,0" />
        <TextBox Height="23" Margin="12,46,12,0" x:Name="txtFolderName" VerticalAlignment="Top" />
        <Grid Height="29" Margin="12,75,12,0" Name="grid1" VerticalAlignment="Top">
            <Button Content="Cancel" Height="23" Name="btnCancel" VerticalAlignment="Top" IsCancel="True" HorizontalAlignment="Right" Width="75" />
            <Button Content="OK" Height="23" Margin="0,0,81,0" Name="btnCreate" VerticalAlignment="Top" IsDefault="True" HorizontalAlignment="Right" Width="75" Click="btnCreate_Click" />
        </Grid>
    </Grid>
</Window>
